@model UpdateContactDto
@{
	ViewData["Title"] = "İletişim Bilgisi Güncelleme Sayfası";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
	<input type="hidden" asp-for="Id" />
	<label>Telefon</label>
	<input type="text" asp-for="Phone" class="form-control" />
	<br />
	<label>Email</label>
	<input type="text" asp-for="Email" class="form-control" />
	<br />
	<label>Harita Bilgisi</label>
	<input type="text" asp-for="MapUrl" class="form-control" />
	<br />
	<button class="btn btn-dark">Kaydet</button>
</form>

